    <script>

        function desktopMegaMenuScript() {
            $(".category-list ul").slideDown();
            $(window).mouseover(function (e) {
                if (!$(e.target).closest("#megamenu").length) {
                    $("#megamenu").fadeOut();
                }

            })
            $("#category").mouseover(function (e) {
                e.stopPropagation();
                $("#megamenu").fadeIn();
            })
            $("#megamenu").mouseover(function (e) {
                e.stopPropagation();
            })
            $(".tab-Index").mouseover(function () {
                if ($(".tab-Index").hasClass("active")) {
                    $(".tab-Index").removeClass("active");
                }
                $(this).toggleClass("active");
                var id = $(".tab-Index.active").attr("data-index");
                $(".tab-container-Item").removeClass("active");
                $(".tab-container-Item[data-index='" + id + "']").addClass("active");
            })
        }
        function mobileMegamenuScript() {
            $(".category-list ul").slideUp();
            $("#Category").click(function (e) {
                $("#megamenu").fadeToggle();
            })
            $(".tab-Index").click(function () {
                if ($(".tab-Index").hasClass("active")) {
                    $(".tab-Index").removeClass("active");
                }
                $(this).toggleClass("active");
                var id = $(".tab-Index.active").attr("data-index");
                $(".tab-container-Item").removeClass("active");
                $(".tab-container-Item[data-index='" + id + "']").addClass("active");
            })
            $(".category-list h6").click(function () {
                $(this).next("ul").slideToggle();
            })
        }
        //$("#megamenu").fadeOut(0);

        if (Breakpointcheker(768,false)) {
            desktopMegaMenuScript();
        }

        else {
            mobileMegamenuScript();
      
        }


        $(window).resize(function () {
            if (Breakpointcheker(767, true)) {
                mobileMegamenuScript();
            }
            else {
                desktopMegaMenuScript();
            }
        })

    </script>
    <script>
        var dropdownLi = document.querySelectorAll(".dropdown");
        dropdownLi.forEach((item) => {
            item.addEventListener("mouseover", function () {
                item.classList.add("active");
            })
            item.addEventListener("mouseout", function () {
                item.classList.remove("active");
                var isSelect = false;
                document.querySelectorAll(".category").forEach((item) => {
                    if (item.classList.contains("select")) {
                        isSelect = true;
                    }
                });
                document.querySelectorAll(".category")[0].classList.add("select");
            })
        })
        function toggleDropdown() {
            const dropdown = document.getElementById("myDropdown");
            dropdown.classList.toggle("show");
        }
    </script>
